{
	"name": "@zaydek/prose",
	"version": "0.2.6",
	"main": "dist/index.css",
	"license": "MIT",
	"files": [
		"dist/"
	],
	"scripts": {
		"watch-prose": "watch --interval 0.1 'yarn --silent sass --no-charset src/index.scss dist/index.css' src",
		"build": "rm -rf dist && yarn sass --no-charset src/index.scss dist/index.css",
		"publish-dist-dry-run": "yarn build && npm publish --dry-run"
	},
	"devDependencies": {
		"@zaydek/duomo": "^0.6.12",
		"prettier": "^2.1.2",
		"sass": "^1.28.0",
		"watch": "^1.0.2"
	}
}
